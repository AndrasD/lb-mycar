{
  "name": "Coordinate",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "simcardId": {
      "type": "string",
      "required": true,
      "index": true
    },
    "point": {
      "type": "string",
      "required": true,
      "length": 21
    },
    "time": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "simcard": {
      "type": "belongsTo",
      "model": "Simcard",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }   
  ],
  "methods": {}
